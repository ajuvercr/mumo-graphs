<> a tree:Condition;
  tree:relationType tree:EqualToRelation;
  tree:value <https://heron.libis.be/momu-test/api/items/16773>;
  tree:path (
    sosa:madeBySensor
    [ sh:inversePath sosa:hosts ]
    cidoc:P55_has_current_location
    [ sh:zeroOrMorePath dcterms:isPartOf ]
  ).

